  resonance_formula: |
    function calculateResonance(f1, f2) {
      const ratio = Math.max(f1, f2) / Math.min(f1, f2);
      const harmonics = [1, 2, 3/2, 4/3, 5/4, 6/5, 8/5, 5/3];
      const closest = harmonics.reduce((p, c) => 
        Math.abs(c - ratio) < Math.abs(p - ratio) ? c : p
      );
      return 100 * (1 - Math.abs(ratio - closest) / closest);
    }
    
